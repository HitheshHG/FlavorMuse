{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/ayur/:path*",
      "destination": "/api/ayur/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/ayur/**": {
      "maxDuration": 15
    }
  }
}